{
  "version": 2,
  "name": "logo-declinaisons-trial",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/trial/check",
      "dest": "/api/trial/check.js"
    },
    {
      "src": "/api/trial/increment",
      "dest": "/api/trial/increment.js"
    },
    {
      "src": "/api/trial/reset",
      "dest": "/api/trial/reset.js"
    },
    {
      "src": "/api/license/activate",
      "dest": "/api/license/activate.js"
    },
    {
      "src": "/api/license/validate",
      "dest": "/api/license/validate.js"
    },
    {
      "src": "/api/license/deactivate",
      "dest": "/api/license/deactivate.js"
    },
    {
      "src": "/api/license/force-deactivate",
      "dest": "/api/license/force-deactivate.js"
    },
    {
      "src": "/api/webhooks/lemonsqueezy",
      "dest": "/api/webhooks/lemonsqueezy.js"
    },
    {
      "src": "/api/debug/redis",
      "dest": "/api/debug/redis.js"
    },
    {
      "src": "/api/version/latest",
      "dest": "/api/version/latest.js"
    },
    {
      "src": "/api/updates/manifest",
      "dest": "/api/updates/manifest.js"
    },
    {
      "src": "/api/updates/files",
      "dest": "/api/updates/files.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
